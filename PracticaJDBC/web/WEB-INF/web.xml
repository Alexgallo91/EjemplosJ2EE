<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Principal</servlet-name>
        <servlet-class>Servlets.Principal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderIDEmpleado</servlet-name>
        <servlet-class>Servlets.OrderIDEmpleado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>Servlets.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cambio</servlet-name>
        <servlet-class>Servlets.SimularCambios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderNombre</servlet-name>
        <servlet-class>Servlets.OrderNombre</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderSalario</servlet-name>
        <servlet-class>Servlets.OrderSalario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderDepartamento</servlet-name>
        <servlet-class>Servlets.OrderDepartamento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Actualizar</servlet-name>
        <servlet-class>Servlets.Actualizar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Simular</servlet-name>
        <servlet-class>Servlets.Simular</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Principal</servlet-name>
        <url-pattern>/Principal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderIDEmpleado</servlet-name>
        <url-pattern>/OrderIDEmpleado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cambio</servlet-name>
        <url-pattern>/cambio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderNombre</servlet-name>
        <url-pattern>/OrderNombre</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderSalario</servlet-name>
        <url-pattern>/OrderSalario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderDepartamento</servlet-name>
        <url-pattern>/OrderDepartamento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Actualizar</servlet-name>
        <url-pattern>/Actualizar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Simular</servlet-name>
        <url-pattern>/Simular</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
